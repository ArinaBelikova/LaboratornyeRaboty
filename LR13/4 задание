#include <stdio.h>
#include <locale.h>

int main()
{
	setlocale(LC_ALL, "Rus");
  float A, k=1;
	printf("Введите вещественнойе число ");
  scanf ("%f", &A);
	int N;
	printf("Введите целое число ");
	scanf ("%i", &N);
	for  (int i = 1; i <= N; i++){
    	k *= A;
    	printf("%f \n", k);
	}
	return 0;
}
