#include<stdio.h>
#include<math.h>
#include <locale.h>

int main()
{
	setlocale(LC_ALL, "Rus");
	float a, b, c, d;
	printf(" Введите 2 ненулевых числа\n");
	scanf(" %f  %f", &a, &b); 
	c=fabs(a);
	d=fabs(b);
	float S = c+d; 
	float R = c-d;
	float P = c*d;
	float D = c/d;
	printf(" Сумма: %f", S); 
	printf(" Разность %f", R);
	printf(" Произведение %f", P);
	printf(" Частное %f", D);
	return 0; 
}
