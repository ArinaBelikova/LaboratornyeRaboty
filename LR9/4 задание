#include <stdio.h>
#include <locale.h>

int main()
{
	  setlocale(LC_ALL, "Rus");
    int A,B,C, S, K;
    printf("Введите стороны прямоугольника\n");
    scanf ("%i  %i", &A, &B);
    printf("Введите сторону квадрата\n");
    scanf ("%i", &C);
    K=(A/C)*(B/C);
    S=A*B-K*C*C;
    printf("Колличество квадратов в прямоугольнике: %i\nПлощадь незанятой части: %i", K, S);
	  return 0;
}
