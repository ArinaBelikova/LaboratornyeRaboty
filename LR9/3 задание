#include <stdio.h>
#include <locale.h>
 
int main()
{
	  setlocale(LC_ALL, "Rus");
    int K, N;
    printf("Введите целое число, лежащее в диапазоне 1-365\n");
    scanf ("%i", &K);
    printf("Введите целое число, лежащее в диапазоне 1-7\n");
    scanf ("%i", &N);
    printf("%i\n",((K+N-2)%7)+1);
    return 0;
}
