#include<stdio.h>
#include<math.h>
#include <locale.h>

int main()
{
	setlocale(LC_ALL, "Rus");
	float x1, y1, x2, y2;
	printf("Введите координаты 1 точки\n");
	scanf("%f ", &x1);
	scanf("%f ", &y1);
	printf("Введите координаты 2 точки\n");
	scanf("%f ", &x2);
	scanf("%f ", &y2);
	float a = pow(x2-x1, 2);
	float b = pow(y2-y1, 2);
	float d = sqrt(a+b);
	printf("%f ", d);
	return 0;
}
